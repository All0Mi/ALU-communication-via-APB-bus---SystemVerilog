v0x5643cc2d7b30_0 .net "error", 0 0, v0x5643cc2d7720_0;  1 drivers
v0x5643cc2d7c30_0 .net/s "result", 7 0, v0x5643cc2d77f0_0;  1 drivers
S_0x5643cc283cf0 .scope module, "bch" "bit_change" 3 14, 4 1 0, S_0x5643cc2bf250;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "A";
    .port_info 1 /INPUT 8 "B";
    .port_info 2 /OUTPUT 8 "result";
    .port_info 3 /OUTPUT 1 "error";
P_0x5643cc283ed0 .param/l "N" 0 4 1, +C4<00000000000000000000000000001000>;
v0x5643cc284030_0 .net "A", 7 0, v0x5643cc2d7980_0;  1 drivers
v0x5643cc2d7640_0 .net "B", 7 0, v0x5643cc2d7a60_0;  1 drivers
v0x5643cc2d7720_0 .var "error", 0 0;
v0x5643cc2d77f0_0 .var "result", 7 0;
E_0x5643cc2bd9a0 .event edge, v0x5643cc2d7640_0, v0x5643cc284030_0;
    .scope S_0x5643cc283cf0;
T_0 ;
Ewait_0 .event/or E_0x5643cc2bd9a0, E_0x0;
    %wait Ewait_0;
    %load/vec4 v0x5643cc2d7640_0;
    %pad/u 32;
    %cmpi/u 8, 0, 32;
    %flag_inv 5; GE is !LT
    %flag_mov 8, 5;
    %load/vec4 v0x5643cc2d7640_0;
    %pad/u 32;
    %cmpi/u 0, 0, 32;
    %flag_or 5, 8;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5643cc2d7720_0, 0, 1;
    %load/vec4 v0x5643cc284030_0;
    %store/vec4 v0x5643cc2d77f0_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5643cc2d7720_0, 0, 1;
    %load/vec4 v0x5643cc284030_0;
    %pushi/vec4 1, 0, 8;
    %ix/getv 4, v0x5643cc2d7640_0;
    %shiftl 4;
    %or;
    %store/vec4 v0x5643cc2d77f0_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5643cc2bf250;
T_1 ;
    %vpi_call/w 3 22 "$dumpfile", "./testbench/sim/bit_change.vcd" {0 0 0};
    %vpi_call/w 3 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5643cc2bf250 {0 0 0};
    %vpi_call/w 3 25 "$display", "\012Test modu\305\202u bit_change\012" {0 0 0};
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x5643cc2d7980_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5643cc2d7a60_0, 0, 8;
    %delay 10, 0;
    %vpi_call/w 3 30 "$display", "A: %b", v0x5643cc2d7980_0 {0 0 0};
    %vpi_call/w 3 31 "$display", "B: %d", v0x5643cc2d7a60_0 {0 0 0};
    %vpi_call/w 3 32 "$display", "result: %b", v0x5643cc2d7c30_0 {0 0 0};
    %vpi_call/w 3 33 "$display", "error: %b\012", v0x5643cc2d7b30_0 {0 0 0};
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x5643cc2d7980_0, 0, 8;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x5643cc2d7a60_0, 0, 8;
    %delay 10, 0;
    %vpi_call/w 3 38 "$display", "A: %b", v0x5643cc2d7980_0 {0 0 0};
    %vpi_call/w 3 39 "$display", "B: %d", v0x5643cc2d7a60_0 {0 0 0};
    %vpi_call/w 3 40 "$display", "result: %b", v0x5643cc2d7c30_0 {0 0 0};
    %vpi_call/w 3 41 "$display", "error: %b\012", v0x5643cc2d7b30_0 {0 0 0};
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x5643cc2d7980_0, 0, 8;
    %pushi/vec4 254, 0, 8;
    %store/vec4 v0x5643cc2d7a60_0, 0, 8;
