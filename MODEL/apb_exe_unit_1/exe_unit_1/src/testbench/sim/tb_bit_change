S_0x5643cc2bf0c0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5643cc2bf250 .scope module, "testbench" "testbench" 3 4;
 .timescale 0 0;
P_0x5643cc2bf3e0 .param/l "N" 0 3 6, +C4<00000000000000000000000000001000>;
v0x5643cc2d7980_0 .var/s "a", 7 0;
